<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.slu.mapper.MemberMapper">
	<select id="getTime" resultType="string">
		select now();
	</select>

	<insert id="insertMember">
		insert into tbl_member2 (userid,email) values
		(#{userid}, #{email})
	</insert>

	<select id="selectMember" resultType="com.slu.domain.MemberVO">
		select
		*
		from
		tbl_member2
		where userid = #{userid}
	</select>

</mapper>